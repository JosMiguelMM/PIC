.................... char kbd_getc( ) 
.................... { 
....................    static BYTE kbd_call_count; 
....................    static short int kbd_down; 
....................    static char last_key; 
....................    static BYTE col; 
....................    BYTE kchar; 
....................    BYTE row; 
....................    port_b_pullups(TRUE); 
....................    kchar='\0'; 
....................  
....................    if(++kbd_call_count>KBD_DEBOUNCE_FACTOR) 
....................    { 
....................        switch (col) 
....................        { 
....................          case 0   : set_tris_b(ALL_PINS&~COL0); 
....................                     portkbd=(0b1110); 
....................                     break; 
....................  
....................          case 1   : set_tris_b(ALL_PINS&~COL1); 
....................                     portkbd=(0b1101); 
....................                     break; 
....................  
....................          case 2   : set_tris_b(ALL_PINS&~COL2); 
....................                     portkbd=(0b1011); 
....................                     break; 
....................  
....................          case 3   : set_tris_b(ALL_PINS&~COL3); 
....................                     portkbd=(0b0111); 
....................                     break; 
....................        } 
....................  
....................        if(kbd_down) 
....................        { 
....................          if((portkbd & (ALL_ROWS))==(ALL_ROWS)) 
....................          { 
....................            kbd_down=FALSE; 
....................            kchar=last_key; 
....................            last_key='\0'; 
....................          } 
....................        } 
....................        else 
....................        { 
....................           if((portkbd & (ALL_ROWS))!=(ALL_ROWS)) 
....................           { 
....................              if((portkbd & ROW0) == 0) 
....................                row = 0; 
....................              else if((portkbd & ROW1) == 0) 
....................                row = 1; 
....................              else if((portkbd & ROW2) == 0) 
....................                row = 2; 
....................              else if((portkbd & ROW3) == 0) 
....................                row = 3; 
....................              last_key = KEYS[row][col]; 
....................              kbd_down = TRUE; 
....................           } 
....................           else 
....................           { 
....................              ++col; 
....................              if(col==4) 
....................              col=0; 
....................           } 
....................        } 
....................       kbd_call_count=0; 
....................    } 
....................   kchar = last_key; 
....................   set_tris_b(ALL_PINS); 
....................   return(kchar); 
.................... } 
